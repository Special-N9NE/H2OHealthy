<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="progressValue" format="integer" />
    <attr name="progressColor" format="color" />
    <attr name="progressBackgroundColor" format="color" />
    <attr name="textColor" format="color" />
    <attr name="textSize" format="dimension" />
    <attr name="thickness" format="dimension" />
    <attr name="radius" format="dimension" />
    <attr name="animationDuration" format="integer" />
    <attr name="textVisibility" format="enum">
        <enum name="gone" value="8" />
        <enum name="invisible" value="4" />
        <enum name="visible" value="0" />
    </attr>
    <attr name="font" format="reference" />
    <attr name="gradient" format="boolean" />
    <attr name="gradientStartColor" format="color" />
    <attr name="gradientEndColor" format="color" />

    <declare-styleable name="BaseProgressBar">
        <attr name="progressValue" />
        <attr name="progressColor" />
        <attr name="progressBackgroundColor" />
        <attr name="textSize" />
        <attr name="textColor" />
        <attr name="thickness" />
        <attr name="radius" />
        <attr name="animationDuration" />
        <attr name="textVisibility" />
        <attr name="font" />
        <attr name="gradient" />
        <attr name="gradientStartColor" />
        <attr name="gradientEndColor" />
    </declare-styleable>
</resources>